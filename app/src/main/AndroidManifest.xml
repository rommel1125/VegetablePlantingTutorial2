<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vegetableplantingtutorial">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_vege_plant_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_vege_plant_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VegetablePlantingTutorial">
        <activity android:name=".TutorialActivity" android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".ListdataActivity"
            android:parentActivityName=".ListdataActivity" />
        <activity
            android:name=".MainActivity"
            android:label="Vegetable Planting"
            android:theme="@style/Theme.VegetablePlantingTutorial.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>